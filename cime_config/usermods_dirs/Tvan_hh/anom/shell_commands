./xmlchange RUN_STARTDATE=2022-01-01
./xmlchange DATM_YR_ALIGN=2008,DATM_YR_END=2021,DATM_YR_START=2008

# Set up a simulation from cold start
./xmlchange CLM_FORCE_COLDSTART=off
./xmlchange CLM_ACCELERATED_SPINUP=off
./xmlchange STOP_N=26 
./xmlchange REST_N=26
./xmlchange RESUBMIT=2
./xmlchange RUN_REFDATE=2022-01-01
./xmlchange RUN_STARTDATE=2022-01-01
./xmlchange CONTINUE_RUN=FALSE
./xmlchange CLM_CO2_TYPE=diagnostic
./xmlchange DATM_CO2_TSERIES=SSP3-7.0
